    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "anotherdev": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack --watch --mode development --progress & npx tailwindcss -i ./src/input.css -o ./tailwind.css --watch",
